{
  "name": "astroffers",
  "version": "0.0.7",
  "description": "Take offers to watch at given nights by the NGC2000 catalog",
  "main": "lib/main.js",
  "typings": "lib",
  "scripts": {
    "build": "rimraf lib && tsc",
    "build:w": "rimraf lib && tsc -w",
    "start": "electron lib/main",
    "start-prod": "NODE_ENV=production npm start",
    "publish": "electron-builder --win --mac --linux --publish always",
    "mac-test": "npm run build && electron-builder --mac --dir"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayhasyee/astroffers.git"
  },
  "keywords": [],
  "author": "PÃ©ter Hauszknecht",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jayhasyee/astroffers/issues"
  },
  "homepage": "https://github.com/jayhasyee/astroffers",
  "dependencies": {
    "classnames": "^2.2.5",
    "electron-log": "^2.2.11",
    "electron-updater": "^2.16.1",
    "font-roboto-local": "^1.0.1",
    "highcharts": "^6.0.3",
    "highcharts-more": "^0.1.2",
    "left-pad": "^1.2.0",
    "material-ui": "^0.19.4",
    "mdi": "^2.0.46",
    "moment": "^2.19.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-google-maps": "^9.2.2",
    "react-highcharts": "^15.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "repatch": "^1.3.12",
    "suncalc": "^1.8.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.46",
    "@types/react": "^16.0.22",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.12",
    "electron": "^1.7.9",
    "electron-builder": "^19.46.4",
    "rimraf": "^2.6.2",
    "typescript": "^2.5.3"
  },
  "build": {
    "productName": "Astroffers",
    "appId": "org.electron.astroffers",
    "publish": {
      "provider": "github",
      "owner": "jayhasyee",
      "repo": "astroffers",
      "releaseType": "release"
    },
    "mac": {
      "category": "public.app-category.astronomy"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "category": "Astronomy",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
